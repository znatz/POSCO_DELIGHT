}
/*ドロップダウンメニュー*/
ul ul {
	position: absolute;
	display: none;
}

li:hover ul, li:target ul {
	display: block;
}

ul:target {
	float: none;
	display: block;
}

li:hover ul li, li:target ul li {
	float: none;
	display: block;
}

li ul li a {
	font-size: 12px;
}